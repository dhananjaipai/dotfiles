{
  "[dockerfile]": {
    "editor.defaultFormatter": "ms-azuretools.vscode-docker"
  },
  "[dotenv][env][shellscript][ignore]": {
    "djpai.format.command": "shfmt \"{file}\"",
    "djpai.format.mode": "inline_file_stdout",
    "editor.defaultFormatter": "djpai.faster-format-with-cli"
  },
  "[helm]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": false
  },
  "[javascript]": {
    "editor.defaultFormatter": "djpai.faster-format-with-cli"
  },
  "[jinjaform]": {
    "djpai.format.command": "sed '/^$/N;/^\\n$/D' | sed -e 's/{%/#{%/g' | terraform fmt -write=false - | sed -e 's/#{%/{%/g'",
    "djpai.format.mode": "inline_stdin",
    "editor.defaultFormatter": "djpai.faster-format-with-cli"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[just]": {
    "djpai.format.command": "just --fmt --unstable --justfile \"{file}\"",
    "djpai.format.mode": "inline_file"
  },
  "[markdown]": {
    "editor.defaultFormatter": "yzhang.markdown-all-in-one"
  },
  "[packer]": {
    "djpai.format.command": "sed '/^$/N;/^\\n$/D' | packer fmt -",
    "djpai.format.mode": "inline_stdin",
    "editor.defaultFormatter": "djpai.faster-format-with-cli"
  },
  "[plaintext]": {
    "djpai.format.command": "cat \"{file}\"",
    "djpai.format.mode": "inline_file",
    "editor.defaultFormatter": "djpai.faster-format-with-cli",
    "editor.formatOnSave": false
  },
  "[powershell]": {
    "editor.defaultFormatter": "ms-vscode.powershell"
  },
  "[terraform-vars]": {
    "editor.defaultFormatter": "hashicorp.terraform"
  },
  "[terraform]": {
    "djpai.format.command": "sed '/^$/N;/^\\n$/D' | terraform fmt -",
    "djpai.format.mode": "inline_stdin",
    "editor.defaultFormatter": "djpai.faster-format-with-cli"
  },
  "[toml]": {
    "editor.defaultFormatter": "tamasfe.even-better-toml"
  },
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "code-runner.runInTerminal": false,
  "color-highlight.markerType": "background",
  "color-highlight.markRuler": false,
  "command-runner.commands": {
    "copycommand": "cc",
    "rgf": "rgf"
  },
  "conventionalCommits.gitmoji": false,
  "conventionalCommits.lineBreak": "\\n",
  "conventionalCommits.promptFooter": false,
  "cSpell.enabled": false,
  "cSpell.ignoreWords": [],
  "cSpell.userWords": [],
  "debug.inlineValues": "on",
  "diffEditor.ignoreTrimWhitespace": false,
  "diffEditor.useInlineViewWhenSpaceIsLimited": false,
  "djpai.format.command": "npx prettier --ignore-unknown \"{file}\"",
  "djpai.format.mode": "inline_file_stdout",
  "editor.colorDecorators": false,
  "editor.copyWithSyntaxHighlighting": false,
  "editor.fontFamily": "'JetBrainsMono Nerd Font Mono', Menlo, Monaco, 'Courier New', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 14,
  "editor.formatOnPaste": false,
  "editor.formatOnSave": false,
  "editor.formatOnType": false,
  "editor.insertSpaces": true,
  "editor.lineHeight": 0,
  "editor.minimap.renderCharacters": false,
  "editor.renderControlCharacters": true,
  "editor.stickyScroll.enabled": false,
  "editor.tabSize": 2,
  "editor.tokenColorCustomizations": {},
  "editor.unicodeHighlight.ambiguousCharacters": true,
  "editor.unicodeHighlight.invisibleCharacters": true,
  "emmet.includeLanguages": {
    "javascript": "javascriptreact"
  },
  "errorLens.enabled": false,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.fileNesting.patterns": {
    "*.db": "${capture}.${extname}-*",
    "*.db3": "${capture}.${extname}-*",
    "*.js": "${capture}.js.map, ${capture}.min.js, ${capture}.d.ts",
    "*.jsx": "${capture}.js",
    "*.s3db": "${capture}.${extname}-*",
    "*.sdb": "${capture}.${extname}-*",
    "*.sqlite": "${capture}.${extname}-*",
    "*.sqlite3": "${capture}.${extname}-*",
    "*.ts": "${capture}.js",
    "*.tsx": "${capture}.ts",
    "package.json": "package-lock.json, yarn.lock, pnpm-lock.yaml, bun.lockb",
    "tsconfig.json": "tsconfig.*.json"
  },
  "extensions.ignoreRecommendations": true,
  "files.associations": {
    "*.j2": "jinjaform",
    ".env*": "env",
    "editor.foldingStrategy": "auto"
  },
  "files.defaultLanguage": "${activeEditorLanguage}",
  "git.autofetch": true,
  "git.confirmSync": false,
  "git.email": "${env:GIT_COMMITTER_EMAIL}",
  "git.enableSmartCommit": true,
  "git.name": "${env:GIT_COMMITTER_NAME}",
  "git.openRepositoryInParentFolders": "never",
  "git.smartCommitChanges": "all",
  "github.copilot.enable": {
    "*": true,
    "markdown": false,
    "plaintext": false,
    "scminput": false
  },
  "gitlens.changes.locations": [
    "gutter",
    "line"
  ],
  "gitlens.codeLens.enabled": false,
  "gitlens.gitCommands.closeOnFocusOut": true,
  "gitlens.keymap": "none",
  "gitlens.mode.active": "review",
  "gitlens.mode.statusBar.enabled": false,
  "gitlens.statusBar.enabled": false,
  "gitlens.terminalLinks.enabled": false,
  "hadolint.hadolintPath": "/usr/local/bin/hadolint",
  // "http.proxy": "http://192.168.1.243:8080",
  // "http.proxyStrictSSL": false,
  "indentRainbow.colors": [
    "rgba(29, 78, 137,0.15)",
    "rgba(58, 178, 202,0.15)",
    "rgba(125, 207, 182, 0.15)",
    "rgba(251, 209, 162,0.15)",
    "rgba(247, 146, 86,0.15)",
    "rgba(247, 146, 86,0.1)",
    "rgba(247, 146, 86,0.05)"
  ],
  "javascript.updateImportsOnFileMove.enabled": "never",
  "Notes.notesLocation": "/Users/a1079774/Downloads/scratchpad",
  "remote.extensionKind": {},
  "security.workspace.trust.untrustedFiles": "open",
  "selectby.regexes": {
    "markdown_code": {
      "backward": "```(.*)\n",
      "backwardInclude": false,
      "flags": "i",
      "forward": "```",
      "forwardInclude": false
    },
    "markdown_column": {
      "backward": "\\|(\\s)*",
      "backwardInclude": false,
      "flags": "i",
      "forward": "(\\s)*\\|",
      "forwardInclude": false
    }
  },
  "sqltools.useNodeRuntime": true,
  "telemetry.telemetryLevel": "off",
  "terminal.integrated.enableMultiLinePasteWarning": "never",
  "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Mono",
  "terminal.integrated.fontSize": 13,
  "terraform.languageServer.enable": true,
  "window.newWindowDimensions": "maximized",
  "window.zoomLevel": -1,
  "workbench.activityBar.location": "top",
  "workbench.colorCustomizations": {
    "[One Monokai]": {
      "editor.selectionBackground": "#6B717D"
    }
  },
  "workbench.colorTheme": "One Monokai",
  // Add custom patterns for file names instead of static names
  "workbench.editor.customLabels.patterns": {
    "*/**/index.html": "${dirname}/",
    "*/**/main.tf": "${dirname}/main",
    "*/**/outputs.tf": "${dirname}/outputs",
    "*/**/variables.tf": "${dirname}/variables"
  },
  "workbench.iconTheme": "material-icon-theme",
  "workbench.productIconTheme": "fluent-icons",
  "workbench.startupEditor": "none",
  "chat.agent.enabled": true,
  "mcp": {
    "servers": {
        "Context7": {
          "type": "stdio",
          "command": "npx",
          "args": ["-y", "@upstash/context7-mcp@latest"]
        }
    }
  },
}
